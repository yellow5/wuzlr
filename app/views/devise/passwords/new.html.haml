- @subheader = "Forgot your password?"
= semantic_form_for(resource, :as => resource_name, :url => password_path(resource_name), :html => { :method => :post }) do |f|
  = f.error_messages
  = f.inputs do
    = f.input :email
  = f.buttons do
    = f.commit_button "Send instructions"
= render :partial => "devise/shared/links"
