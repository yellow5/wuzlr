- @subheader = "Resend unlock instructions"
= semantic_form_for(resource, :as => resource_name, :url => unlock_path(resource_name), :html => { :method => :post }) do |f|
  = f.error_messages
  - f.inputs do
    = f.input :email
  - f.buttons do
    = f.commit_button "Resend unlock instructions"
= render :partial => "devise/shared/links"
